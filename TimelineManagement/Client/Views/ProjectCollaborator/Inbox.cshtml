@{
    ViewData["Title"] = "Inbox";
    Layout = "_LayoutWaitingInvitation";
}
<div class="container-xxl flex-grow-1 container-p-y">
    <div class="row">
        <div class="col-md-12">
            <div class="card mb-4">
                <h1 class="textProject mt-3">Collaboration Invitation</h1>

                <div class="container-xxl flex-grow-1 container-p-y">
                    <table class="table table-hover">
                        <thead>
                        <tr>
                            <th>Project Name</th>
                            <th>Status</th>
                            <th>Employee Name</th>
                            <th>Action</th>
                        </tr>
                        </thead>
                        <tbody id="tbodyInbox">
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

