@{
    ViewData["Title"] = "Staff Data";
    Layout = "_LayoutStaffData";
}

<h1 class="textProject mt-3">Staff Data</h1>

<div class="container-xxl flex-grow-1 container-p-y">
    <table id="staffTable" class="display">
    <thead>
        <tr>
            <th>No</th>
            <th>Name</th>
            <th>Birth Date</th>
            <th>Gender</th>
            <th>Hiring Date</th>
            <th>Email</th>
            <th>PhoneNumber</th>
        </tr>
    </thead>
    <tbody id="tbodyStaff">
    </tbody>
</table>
</div>

